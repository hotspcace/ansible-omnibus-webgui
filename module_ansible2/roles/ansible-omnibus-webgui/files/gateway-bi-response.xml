<?xml version='1.0' encoding='UTF-8'?>
<agent-input>
  <variables>
    <variable name='sharedLocation' value='/opt/IBM/IBMIMShared'/>
  </variables>
  <server>
    <repository location='/tmp/install/core/OMNIbusRepository'/>
    <repository location='/tmp/install/gateway/im-nco-g-file-3_1.zip'/>

  </server>
  <profile id='IBM Netcool Core Components' installLocation='/opt/IBM/tivoli/netcool'>
    <data key='cic.selector.arch' value='x86_64'/>
    <data key='user.migratedata,com.ibm.tivoli.omnibus.core' value='false'/>
  </profile>
  <install>
    <!-- IBM Tivoli Netcool/OMNIbus 8.1.0.5 -->
    <!--<offering profile='IBM Netcool Core Components' id='com.ibm.tivoli.omnibus.core' version='5.50.49.20151123_1445' features='nco_core_feature,nco_admin_gui_feature,nco_admin_tools_feature,nco_tec_migration,nco_operator_gui_feature,nco_objserv_feature,nco_g_objserv_feature,nco_bridgeserv_feature,nco_proxyserv_feature,nco_pa_feature,nco_probes_support_feature,nco_gateways_support_feature,nco_mib_manager_feature,nco_extensions_feature'/>-->
    <offering profile='IBM Netcool Core Components' id='com.ibm.tivoli.omnibus.core' version='5.50.49.20151123_1445' features='nco_gateways_support_feature'/>
    <!-- Netcool/OMNIbus Gateway nco-g-file 1.3.1.0 -->
    <offering profile='IBM Netcool Core Components' id='com.ibm.tivoli.omnibus.integrations.nco-g-file' version='1.3.1.3' features='nco-g-file'/>
  </install>
  <preference name='com.ibm.cic.common.core.preferences.eclipseCache' value='${sharedLocation}'/>
</agent-input>
